#!/bin/bash
#PBS -l nodes=1:ppn=1:dc2,walltime=01:00:00
#PBS -N app-freesurfer-post
#PBS -V

## set up environment for HPC
if [ $ENV == "IUHPC" ]; then
    cd $PBS_O_WORKDIR
    module load fsl
    module load freesurfer
fi

## run tool
bash ./main.sh

## check if it worked? this just assumes it succeeds
echo 0 > finished

